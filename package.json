{
  "name": "check-is-deprecated",
  "version": "2.0.0",
  "description": "Check if npm package is deprecated or archived",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "main": "bin/index.js",
  "scripts": {
    "prepack": "npm run clean && npm run prettier && npm run build",
    "clean": "rimraf lib/*",
    "build": "tsc -p .",
    "watch": "npm run build -- --watch",
    "prettier": "prettier --write 'lib/**/*.ts'",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awesome-cli/check-is-deprecated.git"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "npm",
    "github",
    "deprecated",
    "archived",
    "repository",
    "package",
    "check",
    "find"
  ],
  "bugs": {
    "url": "https://github.com/awesome-cli/check-is-deprecated/issues"
  },
  "homepage": "https://github.com/awesome-cli/check-is-deprecated#readme",
  "bin": {
    "check-is-deprecated": "bin/index.js",
    "is-deprecated": "bin/index.js",
    "cid": "bin/index.js"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^6.2.0",
    "figlet": "^1.5.0",
    "log-symbols": "^4.0.0",
    "node-fetch": "^2.6.1",
    "npm-get-package-info": "^2.0.2",
    "ora": "^5.1.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.2.0",
    "@types/node": "^14.14.2",
    "@types/node-fetch": "^2.5.7",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "lib/**/*.ts": [
      "npm run prettier"
    ]
  }
}
